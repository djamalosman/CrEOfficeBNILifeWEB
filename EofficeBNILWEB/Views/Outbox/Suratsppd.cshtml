<script type="text/javascript">
	getlog(<?php echo $idsrt ?>)
	function getlog(id) {
		var idx = btoa(id);

		$.ajax({
			url: "<?php echo base_url() ?>outbox/pengajuan_sppd/getloghistori",
			method: "POST",
			dataType: 'json',
			data: { idsurat: idx },
			success: function (msg) {
				var len = Object.keys(msg).length;

				for (var i = 0; i < len; i++) {

					document.getElementById('log_histori').value += "● " + msg[i].TGL_LOG + " - " + msg[i].PEGAWAI + " - " + msg[i].KETERANGAN + " - " + msg[i].KOMENTAR + "\n";

				}

				//$('#exampleModal').modal('show');
			}
		});
	}


</script>

